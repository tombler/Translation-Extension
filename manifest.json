{
  "manifest_version": 2,

  "name": "TOTAL BLOCKage",
  "description": "Tally times visited Facebook or Gmail.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "permissions": [
    "tabs",
    "activeTab",
     "history"
  ]
}